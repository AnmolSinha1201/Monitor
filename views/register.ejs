<h1>Register</h1>
<form method="POST">
    <input type="text" name="email" />
    <input type="password" name="password" />
    <input type="submit" value="Sumbit" />
</form>

<ul>
    <% if (errors) { %>
        <% Object.keys(errors).forEach((key) => { %>
            <% Object.keys(errors[key]).forEach((innerKey) => { %>
                <li><%= `${key} : ${errors[key][innerKey]}` %></li>
            <% }) %>
        <% }) %>
    <% } %>
</ul>


